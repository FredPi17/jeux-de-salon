{
  "name": "flechettes_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 3000",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-path .dockerignore",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore --ignore-path .dockerignore",
    "lintfix:style": "stylelint --fix **/*.{vue,css,scss} --ignore-path .gitignore --ignore-path .dockerignore",
    "up": "taze major -I",
    "set-major": "taze major -w"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pinia": "^2.0.36",
    "sass": "^1.62.1",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0",
    "vue3-lottie": "^2.7.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.7",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.12.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.0",
    "sugarss": "^5.0.0",
    "taze": "^0.10.1",
    "typescript": "~5.0.4",
    "vite": "^6.2.2",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-svg-icons": "^0.1.0",
    "vitest": "^3.0.8",
    "vue-tsc": "^1.6.4"
  }
}
